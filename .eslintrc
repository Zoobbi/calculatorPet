{
  "extends": [
    "airbnb-standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "codeFrame": false
  },
  "rules": {
    "no-plusplus": "off",
    "max-len": ["error", { "code": 150 }],
    "no-unused-vars": "warn",
    "linebreak-style": 0,
    "no-underscore-dangle": 0
  },
 // "eslint linebreak-style": ["error", "windows"]  ,
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": [
    "@typescript-eslint"
  ]
}
